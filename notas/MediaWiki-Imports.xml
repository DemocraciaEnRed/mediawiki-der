      <comment>Use @box-shadow-card</comment>
      <model>sanitized-css</model>
      <format>text/css</format>
      <text xml:space="preserve" bytes="207">.wiki-template-infocard {
	border: 1px solid #ccc;
	box-shadow: 0 1px 1px rgba( 0, 0, 0, 0.15 );
	border-radius: 2px;
	padding: 1em;
}

